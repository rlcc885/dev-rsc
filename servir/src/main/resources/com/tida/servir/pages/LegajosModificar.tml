<html t:type="layout" title="Servir  Legajo"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">

 
      <div id = "Datos_Generales">
           <h2> Legajo</h2>
           <div id= "basicosTrabajador">
               <h3>${trabajador.tipoDocumento}: ${trabajador.nroDocumento}
               ${trabajador.apellidoPaterno}  ${trabajador.apellidoMaterno} ${trabajador.nombres}</h3>
           </div>
      </div>

    <div class="wrap">

    <!-- the tabs -->
        <ul id="tabs-top" class="tabs">
       <li><a href="#"> Legajo</a></li>
     
        </ul>

        <div class="pane">
            <ul id="tabs-Legajos" class="tabs">
                <li><a href="#"> Identificación Personal/Familiar</a></li>
                <li><a href="#"> Datos Personales</a></li>
                <li><a href="#"> Formación</a></li>
                <li><a href="#"> Producción Intelectual</a></li>
                <li><a href="#"> Carrera Laboral</a></li>
                <li><a href="#"> Licencias </a></li>
                <li><a href="#"> Evaluaciones </a></li>
                <li><a href="#"> Méritos y Deméritos </a></li>
                <li><a href="#"> Bonif. y Reten.</a></li>
                <li><a href="#"> Antecedentes Laborales </a></li>
            </ul>

            <div  class="pane" id='legajoIdentificacionPersonalFamiliar' name='legajoIdentificacionPersonalFamiliar'>
              <t:zone t:id="constanciaIdentificacionPersonalFamiliarZone">
                    <t:ConstanciasDocumentalesEditor t:id="constanciaIdentificacionPersonalFamiliarEditor" legajo= "legajo"  titulo = "titIdPers" categoria = "catIDPers" zone="constanciaIdentificacionPersonalFamiliarZone"/>
              </t:zone>
            </div>

            <div class="pane" id='legajoDatosPersonales' name='legajoDatosPersonales'>
              <t:zone t:id="constanciaDatosPersonalesZone">
                    <t:ConstanciasDocumentalesEditor t:id="constanciaDatosPersonalesEditor" legajo= "legajo"  titulo = "TitDatosPers" categoria = "catDatosPers" zone="constanciaDatosPersonalesZone"/>
              </t:zone>
            </div>

            <div  class="pane" id='legajoFormacion' name='legajoFormacion'>
              <t:zone t:id="constanciaFormacionZone">
                    <t:ConstanciasDocumentalesEditor  t:id="constanciaFormacionEditor" legajo= "legajo"  titulo = "TitFormacion" categoria = "catFormacion" zone="constanciaFormacionZone"/>
              </t:zone>
            </div>

            <div class="pane"  id='legajoProduccion' name='legajoProduccion'>
              <t:zone t:id="constanciaProduccionZone">
                    <t:ConstanciasDocumentalesEditor  t:id="constanciaProduccionEditor" legajo= "legajo"  titulo = "TitProduccion" categoria = "catProduccion" zone="constanciaProduccionZone"/>
              </t:zone>
            </div>

            <div class="pane"  id='legajoCarrera' name='legajoCarrera'>
              <t:zone t:id="constanciaCarreraZone">
                    <t:ConstanciasDocumentalesEditor  t:id="constanciaCarreraEditor" legajo= "legajo"  titulo = "TitCarrera" categoria = "catCarrera" zone="constanciaCarreraZone"/>
              </t:zone>
            </div>

            <div class="pane"  id='legajoLicencias' name='legajoLicencias'>
              <t:zone t:id="constanciaLicenciasZone">
                    <t:ConstanciasDocumentalesEditor  t:id="constanciaLicenciasEditor" legajo= "legajo"  titulo = "TitLicencias" categoria = "catLicencias" zone="constanciaLicenciasZone"/>
              </t:zone>
            </div>

            <div class="pane"  id='legajoEvaluaciones' name='legajoEvaluaciones'>
              <t:zone t:id="constanciaEvaluacionesZone">
                    <t:ConstanciasDocumentalesEditor  t:id="constanciaEvaluacionesEditor" legajo= "legajo"  titulo = "TitEvaluaciones" categoria = "catEvaluaciones" zone="constanciaEvaluacionesZone"/>
              </t:zone>
            </div>

            <div class="pane"  id='legajoMeritos' name='legajoMeritos'>
              <t:zone t:id="constanciaMeritosZone">
                    <t:ConstanciasDocumentalesEditor  t:id="constanciaMeritosEditor" legajo= "legajo"  
                    titulo = "TitMeritos" categoria = "catMeritos" zone="constanciaMeritosZone"/>
              </t:zone>
              <br/>
              <t:zone t:id="constanciaDemeritosZone">
                    <t:ConstanciasDocumentalesEditor  t:id="constanciaDemeritosEditor" legajo= "legajo"  
