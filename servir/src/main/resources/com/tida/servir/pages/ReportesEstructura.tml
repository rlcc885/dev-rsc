<html t:type="layout" title="Servir Reportes de Estructura"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">


    <t:Menu/>
        <div class="organismo_titulo">
            Entidad /U. Ejecutora: ${entidadUE.denominacion}
        </div>

    <h2> Reportes Entidades </h2>
        <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:C2" href="#">Cargos por Entidad</a>
        <br/>
        <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:C9" href="#">Detalle de Cargos por Entidad</a>
        <br/>
        <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:C10" href="#">Detalle de Cargos Asignados por Entidad</a>
        <br/>
        <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:D1" href="#">Presupuesto Analítico de Personal</a>
        <br/>
         <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:C12" href="#">Remuneraciones (pdf)</a> , <a t:type="eventLink" t:event="reporteRemuneraciones" t:context="literal:C1212" href="#">Remuneraciones (xls)</a>
         
        <br/>
        <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:D3" href="#">Cuadro de Asignación de Personal</a>
        <br/>
        <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:D4" href="#">Resumen de Entidad (pdf)</a> , <a t:type="eventLink" t:event="reporteResumen" t:context="literal:D44" href="#">Resumen de Entidad (xls)</a>
      
        <br/>
        <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:D5" href="#">Detalle de Conceptos Remunerativos</a>
        <br/>
	<a t:type="eventLink" t:event="reporteEntidad" t:context="literal:A3-UE" href="#">Listado de Personal Discapacitado</a>
        <br/>
	<a t:type="eventLink" t:event="reporteEntidad" t:context="literal:A6-UE" href="#">Listado de Personal que debe presentar Declaración Jurada de Bienes y Rentas</a>
        <br/>
	<a t:type="eventLink" t:event="reporteEntidad" t:context="literal:A7-UE" href="#">Listado de hijos de Trabajadores, por Sexo y Edad</a>
        <br/>
	<a t:type="eventLink" t:event="reporteEntidad" t:context="literal:A8-UE" href="#">Listado de Trabajadores que requieren habilitación profesional</a>
        <br/>
        <a t:type="eventLink" t:event="reporteEntidad" t:context="literal:LUE1" href="#">Listado de Trabajadores laboran además en otra Entidad</a>
        <br/>   

    <h2> Unidades Orgánicas </h2>
            
    <div t:type="zone" t:id="listaUOZone" class="selectform" >
                    <t:form t:id="instruccion" t:zone="listaUOZone">
                        <t:errors/>
                            <select t:type="Select" t:id="unidad_organica" t:value="uo" 
                             onChange="this.form.fire(Tapestry.FORM_PROCESS_SUBMIT_EVENT)" t:blankOption="ALWAYS"
                                model="UnidadesOrganicas" encoder="UnidadesOrganicas" t:validate="required" t:zone="listaUOZone"/>
                    </t:form>
                <div class = "f-wrap">
                    <t:if test="uo">
                        <a t:type="eventLink" t:event="reporteUO" t:context="list:uo.id, 'A3'" href="#">Listado de Personal Discapacitado</a>
                        <br/>
                        <a t:type="eventLink" t:event="reporteUO" t:context="list:uo.id, 'A6'" href="#">Listado de Personal que debe presentar Declación Jurada de Bienes y Rentas</a>
                        <br/>
                        <a t:type="eventLink" t:event="reporteUO" t:context="list:uo.id, 'A7'" href="#">Listado de hijos de Trabajadores, por Sexo y Edad</a>
                        <br/>
                        <a t:type="eventLink" t:event="reporteUO" t:context="list:uo.id, 'C1'" href="#">Cargos por Unidad</a>
                        <br/>
                        <a t:type="eventLink" t:event="reporteUO" t:context="list:uo.id, 'C7'" href="#">Listado de Cargos (CAP/NOCAP)</a>
                        <br/>
                        <a t:type="eventLink" t:event="reporteUO" t:context="list:uo.id, 'C8'" href="#">Listado de Cargos (solo CAP)</a>
                        <br/>
                        <a t:type="eventLink" t:event="reporteUO" t:context="list:uo.id, 'D2'" href="#">Presupuesto de Personal</a>
                        <br/>
			<a t:type="eventLink" t:event="reporteUO" t:context="list:uo.id, 'A8'" href="#">Listado de Trabajadores que requieren habilitación profesional</a>
                        <br/>
                   </t:if>
                </div>
    </div> <!-- listaUOZone -->
</html>
