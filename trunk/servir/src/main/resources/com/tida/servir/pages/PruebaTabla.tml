<html t:type="layout" title="Editar Antecedentes Laborales"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">


        <p>
        <H1>Antecedentes Laborales</H1>
        <br/>


    	<form t:type="form" t:id="antLaboralesEdit">
		<t:errors/>
		<table>
			<thead>
				<tr>
                                    <th>Cargo</th>
                                    <th>Empresa</th>
                                    <th>Funci√≥n</th>
                                    <th>Fecha de ingreso</th>
                                    <th>Fecha de egreso</th>
				</tr>
			</thead>
			<tbody>
				<tr t:type="AjaxFormLoop" t:id = "antecedentes" t:source="actual.ant_Laborales"
                                    t:value="ant_Laborales" t:encoder = "ant_laborales_encoder">
						<t:submitnotifier>
							<td><input t:type="TextField" t:id="cargo" t:value="ant_Laborales.cargo" t:validate="required, maxlength=10" size="10"/></td>
							<td><input t:type="TextField" t:id="empresa" t:value="ant_Laborales.empresa" t:validate="required, maxlength=10" size="10"/></td>
							<td><input t:type="TextField" t:id="funcion" value="ant_Laborales.funcion" t:validate="required"/></td>
							<td><input t:type="DateField" t:id="fec_ingreso" t:value="ant_Laborales.fec_ingreso" t:validate="required" size="10"/></td>
							<td><input t:type="DateField" t:id="fec_egreso" t:value="ant_Laborales.fec_egreso" t:validate="required" size="10"/></td>

							<!-- If optimistic locking is needed then uncomment this next line.
								<t:hidden value="ant_Laborales.version"/>
							 -->
							<td><t:removerowlink>Eliminar</t:removerowlink></td>
						</t:submitnotifier>
					<p:addRow>
						<td colspan="5" style="text-align: right">
							<t:addrowlink>Agregar Nuevo</t:addrowlink>
						</td>
					</p:addRow>
				</tr>
			</tbody>
	 	</table>
	 	<input type="submit" value="Guardar"/>
	</form>

            </p>

</html>