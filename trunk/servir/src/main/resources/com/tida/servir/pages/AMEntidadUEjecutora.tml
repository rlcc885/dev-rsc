                     <div class="f-right">
                         <input t:type="Submit" t:id="reset" value="Limpiar Formulario" class="button"/>  
                     </div>
                     <div class="f-right">
                        <input id="submit" type="submit" value="Enviar" class="button"/>
                     </div>
                </div>
                 
                 
