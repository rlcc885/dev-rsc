<html title="Editar Ubigeos"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">

    <t:content>
        <div class="f-wrap">
            <div class="f-left">
                <label>Grupo Ocupacional</label>
               <select t:type="Select" t:id="cod_grupo_ocupacional" t:value="ocupacional.grupoOcupacional"
                model="beanDatoAuxGrupoOcupacional" encoder="beanDatoAuxGrupoOcupacional"
                onChange="this.form.fire(Tapestry.FORM_PROCESS_SUBMIT_EVENT)"
                t:blankOption="AUTO"
                t:disabled="disabled" t:zone="prop:zoneName"/>
            </div>
            <div class="f-left">
                <label>Categor√≠a Ocupacional</label>
                <select t:type="Select" t:id="cod_cat_ocupacional" t:value="ocupacional.categoriaOcupacional"
                model="beanDatoAuxCategoriaOcupacional" encoder="beanDatoAuxCategoriaOcupacional" t:disabled="disabled"
                onChange="this.form.fire(Tapestry.FORM_PROCESS_SUBMIT_EVENT)"
                t:blankOption="AUTO"
                 t:zone="prop:zoneName"/>
            </div>
        </div>
    </t:content>
</html>
